{
  "hosting": {
    "errorPages": {
      "500": "/public/500.html"
    },
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }
}